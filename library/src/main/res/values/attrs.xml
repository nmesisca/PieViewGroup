<resources>

    <declare-styleable name="PieViewGroup">
        <attr name="pvg_labelTextSize" format="dimension"/>
        <attr name="pvg_legendTextSize" format="dimension"/>
        <attr name="pvg_colorArray" format="reference" />
        <attr name="pvg_legendDrawable" format="reference"/>
        <attr name="pvg_showLabels" format="boolean"/>
        <attr name="pvg_donutRadiusPercent" format="integer"/>
        <attr name="pvg_chartType" format="integer">
            <enum name="PIE" value="0"/>
            <enum name="DONUT" value="1"/>
        </attr>
        <attr name="pvg_legendType" format="integer">
            <enum name="NONE" value="0"/>
            <enum name="SHORT" value="1"/>
            <enum name="FULL" value="2"/>
        </attr>
    </declare-styleable>

</resources>